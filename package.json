{
  "name": "nubis",
  "description": "Project starter for elizaOS",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/src/index.js",
  "module": "dist/src/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "project",
    "elizaos"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Dexploarer/nubis.git"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/src/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@elizaos/cli": "1.4.5",
    "@elizaos/core": "1.4.5",
    "@elizaos/plugin-bootstrap": "1.4.5",
    "@elizaos/plugin-browser": "1.0.3",
    "@elizaos/plugin-discord": "1.2.5",
    "@elizaos/plugin-google-genai": "1.0.2",
    "@elizaos/plugin-knowledge": "1.2.2",
    "@elizaos/plugin-mcp": "1.0.7",
    "@elizaos/plugin-openai": "1.0.11",
    "@elizaos/plugin-sql": "1.4.5",
    "@elizaos/plugin-telegram": "^1.0.10",
    "@elizaos/plugin-web-search": "^0.25.6-alpha.1",
    "@tanstack/react-query": "5.85.5",
    "@types/pg": "^8.15.5",
    "clsx": "2.1.1",
    "pg": "^8.16.3",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "tailwind-merge": "3.3.1",
    "tailwindcss": "4.1.12",
    "zod": "4.1.3"
  },
  "devDependencies": {
    "@cypress/react": "^9.0.1",
    "@cypress/vite-dev-server": "^6.0.3",
    "@testing-library/cypress": "^10.0.3",
    "@types/node": "^20.14.10",
    "@types/react": "19.1.11",
    "@types/react-dom": "19.1.8",
    "@types/uuid": "^9.0.7",
    "@vitejs/plugin-react": "5.0.1",
    "bun-types": "^1.0.0",
    "cypress": "^14.4.1",
    "node-fetch": "^3.3.2",
    "prettier": "3.6.2",
    "tsup": "8.5.0",
    "typescript": "5.9.2",
    "vite": "7.1.3"
  },
  "scripts": {
    "start": "elizaos start",
    "dev": "elizaos dev",
    "build": "tsc --noEmit && vite build && tsup",
    "check": "bun run type-check && bun run format:check",
    "lint": "prettier --write ./src",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "test": "bun run build && bun test src/__tests__ src/plugins/social-raids/__tests__",
    "test:coverage": "bun run build && bun test --coverage src/__tests__ src/plugins/social-raids/__tests__",
    "test:watch": "bun test --watch",
    "test:install": "node scripts/install-test-deps.js",
    "format": "prettier --write ./src",
    "format:check": "prettier --check ./src",
    "check-all": "bun run check && bun run test",
    "check:arch": "bun test src/__tests__/elizaos-architecture-compliance.test.ts",
    "check:types": "tsc --noEmit",
    "cy:open": "bun run test:install && cypress open",
    "cy:run": "bun run test:install && cypress run --component",
    "cy:test": "bun run test:install && cypress run --component --reporter spec",
    "cypress:component": "bun run test:install && cypress run --component",
    "cypress:e2e": "bun run test:install && cypress run --e2e",
    "cypress:open": "bun run test:install && cypress open",
    "test:smoke": "bun test src/__tests__/smoke"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "b165ad83e5f7a21bc1edbd83374ca087e3cd6b33"
}
