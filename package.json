{
  "name": "elizaos-agent-project",
  "description": "ElizaOS AI Agent with community management and social media integration",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/src/index.js",
  "module": "dist/src/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "elizaos",
    "elizaos-project",
    "ai-agent",
    "community",
    "social-media"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/src/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@elizaos/core": "^1.0.0",
    "@elizaos/plugin-bootstrap": "^1.0.0",
    "@elizaos/plugin-sql": "^1.0.0",
    "@elizaos/server": "^1.0.0",
    "@supabase/supabase-js": "^2.56.0",
    "agent-twitter-client": "^0.0.13",
    "bun": "^1.2.21",
    "chalk": "^5.6.0",
    "chokidar": "^4.0.3",
    "commander": "^14.0.0",
    "dotenv": "^16.6.1",
    "js-yaml": "^4.1.0",
    "email-automation": "^1.0.2",
    "fs-extra": "^11.3.1",
    "globby": "^14.1.0",
    "inquirer": "^9.3.7",
    "node-cron": "^3.0.3",
    "ora": "^8.2.0",
    "puppeteer": "^21.0.0",
    "telegraf": "^4.12.2",
    "zod": "^4.1.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.2",
    "@types/js-yaml": "^4.0.9",
    "@types/lru-cache": "^7.10.10",
    "@types/node-cron": "^3.0.0",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@types/sqlite3": "^3.1.11",
    "@vitejs/plugin-react": "^4.7.0",
    "prettier": "3.5.3",
    "telegraf": "^4.16.3",
    "tsup": "8.5.0",
    "typescript": "^5.9.2",
    "vite": "^6.3.5"
  },
  "scripts": {
    "dev": "elizaos dev",
    "start": "elizaos start",
    "build": "tsup",
    "type-check": "tsc --noEmit",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "matrix:plan": "bun run scripts/matrix-runner.ts docs/matrix-examples/nubi-matrix.yaml",
    "test:social-raids": "bun test src/plugins/social-raids/__tests__/",
    "test:social-raids:watch": "bun test src/plugins/social-raids/__tests__/ --watch",
    "test:social-raids:coverage": "bun test src/plugins/social-raids/__tests__/ --coverage",
    "test:social-raids:run": "bun run src/plugins/social-raids/__tests__/run-tests.ts",
    "test:social-raids:unit": "bun test src/plugins/social-raids/__tests__/actions.test.ts src/plugins/social-raids/__tests__/services.test.ts src/plugins/social-raids/__tests__/providers_evaluators.test.ts src/plugins/social-raids/__tests__/evaluators.test.ts",
    "test:social-raids:integration": "bun test src/plugins/social-raids/__tests__/integration.test.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "biome": "biome check src",
    "biome:fix": "biome check --write src",
    "biome:format": "biome format --write src",
    "check": "bun run type-check && bun run lint && bun run biome",
    "check:fix": "bun run lint:fix && bun run biome:fix && bun run biome:format"
  },
  "publishConfig": {
    "access": "public"
  },
  "agentConfig": {
    "pluginType": "elizaos:project:1.0.0",
    "character": "Nubi - AI Community Agent",
    "pluginParameters": {
      "DISCORD_API_TOKEN": {
        "type": "string",
        "description": "Discord bot token for server integration",
        "required": false,
        "sensitive": true
      },
      "OPENAI_API_KEY": {
        "type": "string",
        "description": "OpenAI API key for LLM capabilities",
        "required": false,
        "sensitive": true
      },
      "ANTHROPIC_API_KEY": {
        "type": "string",
        "description": "Anthropic API key for Claude models",
        "required": false,
        "sensitive": true
      }
    }
  },
  "gitHead": "b165ad83e5f7a21bc1edbd83374ca087e3cd6b33"
}
